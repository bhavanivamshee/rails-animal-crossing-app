<h1>Log In</h1>

<% @errors && @errors.each do |e| %>
  <li><%= e %></li>
  <%end%>
<%= form_tag login_path do |f| %>
    <%=label_tag "Username"%>
    <%=text_field_tag :username%>
    <br>
    <%=label_tag "Password"%>
    <%=password_field_tag :password %>
    <br>
    <%=submit_tag "Log In"%>
  <%end%>